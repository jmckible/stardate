%h1 Edit P90X

= form_for @p90x do |f|
  %p= f.date_select :date, :order=>[:month, :day, :year]
  %p= f.text_field  :minutes,  :default=>'minutes'
  %p= f.text_field  :description, :default=>'description'
  %p
    = submit_tag 'Update'
    or
    %span.delete= link_to 'delete', @p90x, :method=>:delete